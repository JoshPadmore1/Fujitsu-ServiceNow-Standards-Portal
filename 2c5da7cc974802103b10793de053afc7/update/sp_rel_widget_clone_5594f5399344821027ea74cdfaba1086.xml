  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */
	data.task =[];
	
	var gr = new GlideRecord('sc_task');
	gr.addQuery('assigned_to', gs.getUserID());
	gr.query()
	
	while(gr.next()){
		var task_data ={};
		task_data.number = gr.getValue('number');
		task_data.state = gr.getValue('state');
		task_data.priority = gr.getValue('priority');
		
		data.task.push(task_data);
	}
	

})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2024-01-30 22:47:03&lt;/sys_created_on&gt;&lt;sys_id&gt;789ba1bd9304821027ea74cdfaba10be&lt;/sys_id&gt;&lt;sys_mod_count&gt;2&lt;/sys_mod_count&gt;&lt;sys_name&gt;My tasks&lt;/sys_name&gt;&lt;sys_package display_value="Employee Center Core" source="sn_hr_sp"&gt;3d1da2705b021200a4656ede91f91ab6&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Employee Center Core"&gt;3d1da2705b021200a4656ede91f91ab6&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_789ba1bd9304821027ea74cdfaba10be&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2024-01-30 23:16:38&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div&gt;
